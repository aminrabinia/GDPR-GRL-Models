  <intentional-element decompositiontype="And" description="" id="9" name="the processing is Not carried out by automated means" type="Task"/>
  <intentional-element decompositiontype="And" description="" id="10" name="Subject can transmit data to another controller without hinderance " type="Task"/>
  <intentional-element decompositiontype="Xor" description="" id="11" name="Paragraph 20.2 (direct transmission)" type="Goal"/>
  <intentional-element decompositiontype="And" description="" id="12" name="Direct transmission is not technically feasible" type="Task"/>
  <intentional-element decompositiontype="And" description="" id="13" name="Personal data can be directly transmitted to another controller" type="Task"/>
  <intentional-element decompositiontype="And" description="" id="14" name="Paragraph 20.3 " type="Goal"/>
  <intentional-element decompositiontype="And" description="" id="15" name="Data transmission is without prejudice to  Article 17" type="Task"/>
  <intentional-element decompositiontype="And" description="" id="16" name="The right does not apply to processing necessary for the performance of a task carried out in the public interest or in the exercise of official authority vested in the controller." type="Task"/>
  <intentional-element decompositiontype="And" description="" id="17" name="The right to data portability does not adversely affect the rights and freedoms of others. " type="Task"/>
  <intentional-element decompositiontype="And" description="" id="18" name="Processing is Not based on consent (Article 6(1).a) " type="Task"/>
  <intentional-element decompositiontype="And" description="" id="19" name="Processing is Not based on point (a) of Article 9(2) " type="Task"/>
  <intentional-element decompositiontype="And" description="" id="20" name="Processing is Not based on a contract (Article 6(1).b)" type="Task"/>
 </element-def>
