  <intentional-element decompositiontype="And" description="" id="2" name="Paragraph 20.1 (The right to receive/transmit personal data)" type="Goal"/>
  <intentional-element decompositiontype="Or" description="" id="3" name="Right to receive personal data" type="Goal"/>
  <intentional-element decompositiontype="And" description="" id="4" name="Data is not provided by the subject" type="Task"/>
